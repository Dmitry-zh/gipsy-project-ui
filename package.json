{
  "name": "project-gipsy-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --max-warnings=0",
    "format": "prettier ./src --write",
    "preview": "vite preview",
    "add": "npm run add:android && npm run add:ios",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android",
    "build": "tsc -b && vite build",
    "build:sync": "npm run build && npm run add && npx cap sync",
    "build:android": "npx cap build android",
    "open:android": "npx cap open android",
    "run:android": "npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@capacitor/ios": "^6.2.0",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.471.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-use": "^17.5.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@eslint/js": "^9.17.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-sort-exports": "^0.9.1",
    "globals": "^15.14.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.6",
    "type-fest": "^4.23.0",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
